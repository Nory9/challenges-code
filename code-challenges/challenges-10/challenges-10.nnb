            "source": [
                "\nfunction migratoryBirds(arr) {\n    let freq=[0,0,0,0,0,0];\n    let ans=0,maxi=0;\n    for(let i=0;i<arr.length;i++){\n        freq[arr[i]]++;\n    }\n    for(let i=0;i<freq.length;i++){\n        if(freq[i]>maxi){\n        maxi=freq[i];\n        ans=i;\n        }\n    }\n    return ans;\n\n}"
